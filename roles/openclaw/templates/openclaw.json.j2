{
  "gateway": {
    "mode": "local",
    "port": {{ openclaw_port | default(3030) }},
    "auth": {
      "token": "{{ openclaw_admin_token }}"
    },
    "bind": "loopback",
    "http": {
        "endpoints": {
            "chatCompletions": {
                "enabled": true
            }
        }
    }
  },
  "plugins": {
    "entries": {
        {% for plugin in openclaw_plugins_enabled %}
        "{{ plugin }}": {
            "enabled": true
        }{% if not loop.last %},{% endif %}
        {% endfor %}
    }
  },
  "agents": {
    "defaults": {
        "model": {
            "primary": "{{ openclaw_gemini_model }}"
        }
    }
  },
  "auth": {
    "profiles": {
        "google-antigravity:{{ openclaw_google_email }}": {
            "provider": "google-antigravity",
            "mode": "oauth"
        }
    }
  },
  "logging": {
    "level": "info"
  }
}
