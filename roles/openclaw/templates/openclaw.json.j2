{
  "gateway": {
    "mode": "local",
    "port": {{ openclaw_port | default(3030) }},
    "auth": {
      "token": "{{ openclaw_admin_token }}"
    },
    "bind": "loopback"
  },
  "logging": {
    "level": "info"
  },
  "hooks": {
    "enabled": true,
    "token": "{{ rapidpro_auth_token }}",
    "path": "/hooks",
    "mappings": [
      {
        "id": "rapidpro-default",
        "match": {
          "path": "rapidpro/default"
        },
        "action": "agent",
        "sessionKey": "{% raw %}rapidpro:{{payload.contact.urn}}{% endraw %}",
        "messageTemplate": "{% raw %}{{payload.results.message.value}}{% endraw %}"
      }
    ]
  }
}
