{
  "gateway": {
    "mode": "local",
    "port": {{ openclaw_port | default(3030) }},
    "auth": {
      "token": "{{ openclaw_admin_token }}"
    }
  },
  "logging": {
    "level": "info"
  },
  "hooks": {
    "enabled": true,
    "token": "{{ rapidpro_auth_token }}",
    "path": "/hooks",
    "mappings": [
      {
        "id": "rapidpro-default",
        "match": {
          "path": "rapidpro/default"
        },
        "action": "agent",
        "sessionKey": "rapidpro:{{payload.contact.urn}}",
        "messageTemplate": "{{payload.results.message.value}}"
      }
    ]
  }
}
