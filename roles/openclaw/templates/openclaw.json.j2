{
  "gateway": {
    "mode": "local",
    "port": {{ openclaw_port | default(3030) }},
    "auth": {
      "token": "{{ openclaw_admin_token }}"
    },
    "bind": "loopback"
  },
  "llm": {
    "provider": "{{ openclaw_llm_provider | default('gemini') }}",
    "auth": {
      "gemini": {
        "apiKey": "{{ ai_middleware_google_api_key | default(openclaw_gemini_api_key) | default('') }}"
      }
    },
    "defaults": {
      "model": "{{ openclaw_gemini_model | default('gemini-1.5-flash') }}"
    }
  },
  "logging": {
    "level": "info"
  },
  "hooks": {
    "enabled": true,
    "token": "{{ rapidpro_auth_token }}",
    "path": "/hooks",
    "mappings": [
      {
        "id": "rapidpro-default",
        "match": {
          "path": "rapidpro/default"
        },
        "action": "agent",
        "sessionKey": "{% raw %}rapidpro:{{payload.contact.urn}}{% endraw %}",
        "messageTemplate": "{% raw %}{{payload.results.message.value}}{% endraw %}"
      }
    ]
  }
}
